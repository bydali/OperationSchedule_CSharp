<Window x:Class="OperationSchedule_CSharp.RegularTransmitCommand"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OperationSchedule_CSharp"
        mc:Ignorable="d"
        Title="RegularTransmitCommand" Width="800">
    <Grid>
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="30"/>
		</Grid.RowDefinitions>
		<DataGrid>
			<DataGrid.Columns>
				<DataGridTextColumn Header="选择"/>
				<DataGridTextColumn Header="车站"/>
				<DataGridTextColumn Header="定时间隔（分）"/>
			</DataGrid.Columns>
		</DataGrid>
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Grid.Row="1">
			<Button Content="全选"/>
			<Button Content="全不选"/>
			<Button Content="确定"/>
			<Button Content="关闭"/>
		</StackPanel>
	</Grid>
</Window>
